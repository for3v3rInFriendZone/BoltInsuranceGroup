		}
		
		modify($insurance){
			setAmount($insurance.travelInsurancePrice + $insurance.carInsurancePrice);
		}
		
		System.out.println("Cena ---" + $insurance.amount);
		
		if($insurance.persons.numberOfUsers > 5){
